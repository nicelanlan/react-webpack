{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "webpack test for ReactJS",
  "main": "src/index.tsx",
  "scripts": {
    "build:help": "mv dist/index.html dist/template/ && mv dist/config.json ./dist/template/",
    "clean:build": "rimraf dist/template",
    "clean:cache": "rimraf .awcache",
    "prestart": "npm run clean:cache && cross-env NODE_ENV=development webpack --config config/webpack.config.dll.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js --progress --color",
    "test": "cross-env NODE_ENV=development BABEL_ENV=test jest --config jest.config.js --coverage --no-cache",
    "tslint": "./node_modules/.bin/tslint -c tslint.yml 'src/**/*.{ts,tsx}'",
    "build-local": "npm run clean:build && cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js --progress && npm run build:help",
    "build": "npm run clean:build && cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js && npm run build:help"
  },
  "license": "UNLICENSED",
  "private": true,
